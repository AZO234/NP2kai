IDR_VIEW MENU
BEGIN
    POPUP "システム(&S)"
    BEGIN
        MENUITEM "新規ウィンドウ(&N)",          IDM_VIEWWINNEW
        MENUITEM SEPARATOR
        MENUITEM "閉じる(&C)",                  IDM_VIEWWINCLOSE
        MENUITEM "すべて閉じる(&A)",            IDM_VIEWWINALLCLOSE
    END
    POPUP "表示モード(&M)"
    BEGIN
        MENUITEM "レジスタ(&R)",                IDM_VIEWMODEREG
        MENUITEM "セグメント ダンプ(&D)",       IDM_VIEWMODESEG
