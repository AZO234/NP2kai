        MENUITEM "サウンドレジスタ(&S)",        IDM_VIEWMODESND
        MENUITEM SEPARATOR
        MENUITEM "ロック(&L)",                  IDM_VIEWMODELOCK
    END
    POPUP "編集(&E)"
    BEGIN
        MENUITEM "アドレス入力(&A)...",         IDM_SETSEG, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Seg = &CS",                   IDM_SEGCS
        MENUITEM "Seg = &DS",                   IDM_SEGDS
        MENUITEM "Seg = &ES",                   IDM_SEGES
        MENUITEM "Seg = &SS",                   IDM_SEGSS
        MENUITEM "Seg = &TEXT",                 IDM_SEGTEXT
        MENUITEM SEPARATOR
        MENUITEM "リロード",                    IDM_RELOAD, GRAYED
    END
    POPUP "メモリ(&M)", GRAYED
    BEGIN
        MENUITEM "VRAM ページ&0",               IDM_VRAM0
        MENUITEM "VRAM ページ&1",               IDM_VRAM1
        MENUITEM SEPARATOR
        MENUITEM "&A20ライン",                  IDM_A20LINE
    END
END

