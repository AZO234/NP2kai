				$(IO)/gdc.h $(IO)/gdc_cmd.h $(IO)/gdc_sub.h $(IO)/mouseif.h \
				$(IO)/necio.h $(IO)/nmiio.h $(IO)/np2sysp.h $(IO)/pic.h \
				$(IO)/pit.h $(IO)/printif.h $(IO)/serial.h $(IO)/sysport.h \
				$(IO)/upd4990.h

